
<%= link_to 'Create events', event_page_path, :class => "btn-warning" %>

<div class="event">
  <%= form_for :event do |f| %>
<p>
   <%= f.label :eventtitle %><br />
   <%= f.text_field :eventtitle %>
 </p>
 <p>
   <%= f.label :venue %><br />
   <%= f.text_field :venue %>
 </p>
 <p>
   <%= f.label :date %><br />
   <%= f.text_field :date %>
 </p>
 <p>
   <%= f.label :time %><br />
   <%= f.text_field :time %>
 </p>
 <p>
   <%= f.label :description %><br />
   <%= f.text_field :description %>
 </p>
 <p>
   <%= f.label :invitees %><br />
   <%= f.text_field :invitees %>
 </p>
 <p>
   <%= f.label :status %><br />
   <%= f.text_field :status %>
 </p>
 <% end %>
 </div>